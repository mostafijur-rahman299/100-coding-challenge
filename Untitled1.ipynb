   "source": [
    "arr = [7, 3, 4, 5, 5, 6, 2]\n",
    "\n",
    "# Find missing\n",
    "for i in range(1, len(arr)+1):\n",
    "    if i not in arr:\n",
    "        print(i)\n",
    "        \n",
    "# Find repeating\n",
    "n = len(arr)\n",
    "temp = [0] * n\n",
    "for i in range(n):\n",
    "    temp[arr[i] - 1] += 1\n",
    "    \n",
    "print(temp)"
   ]
