    "import heapq\n",
    "\n",
    "heap = []\n",
    "nums = [3,2,1,5,6,4]\n",
    "k = 2\n",
    "\n",
    "for num in nums:\n",
    "    heapq.heappush(heap, num)\n",
    "\n",
    "    if len(heap) > k:\n",
    "        heapq.heappop(heap)\n",
    "\n",
    "print(heap)"
   ]
